{
  "version": 2,
  "builds": [
    {
      "src": "server/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/wsgi.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11.7"
  }
}
